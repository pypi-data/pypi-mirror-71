{
  "name": "@chameleoncloud/jupyterlab_swift",
  "version": "2.0.1",
  "description": "JupyterLab viewer for Swift containers",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "openstack",
    "swift"
  ],
  "homepage": "https://github.com/chameleoncloud/jupyterlab-swift",
  "bugs": {
    "url": "https://github.com/chameleoncloud/jupyterlab-swift/issues"
  },
  "license": "BSD-3-Clause",
  "author": "University of Chicago",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/**/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chameleoncloud/jupyterlab-swift.git"
  },
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "@jupyterlab/docmanager": "^2.0.0",
    "@jupyterlab/docregistry": "^2.0.0",
    "@jupyterlab/filebrowser": "^2.0.0",
    "@jupyterlab/services": "^5.0.0",
    "@jupyterlab/settingregistry": "^2.0.0",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.11.0",
    "base64-js": "^1.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@types/base64-js": "^1.3.0",
    "@types/text-encoding": "^0.0.35",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1",
    "typescript": "~3.9.2"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_swift"
        }
      }
    },
    "schemaDir": "schema"
  }
}
